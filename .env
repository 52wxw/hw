# 数据库配置
MYSQL_ROOT_PWD=Root@123456  # 与deploy/.env保持一致
MYSQL_DATABASE=ai_inspect

# 加密密钥（已生成示例，实际部署建议重新生成）
FERNET_KEY=eXAmPleKey1234567890abcdefghijklmnopqrstuvwxyz=

# 服务地址配置
OLLAMA_URL=http://host.docker.internal:11434  # 容器内访问宿主机Ollama服务
COLLECT_SERVICE_URL=http://collect-service:8001
AI_SERVICE_URL=http://ai-service:8002
BLOCKCHAIN_URL=http://blockchain:8545

# 区块链配置（可选，暂时用默认值）
BLOCKCHAIN_CONTRACT_ADDRESS=default_contract
BLOCKCHAIN_PRIVATE_KEY=default_private_key

# 告警服务配置（可选，暂时留空）
# 告警服务配置（正确示例）
SMTP_HOST=smtp.qq.com
SMTP_PORT=587  # 必须是整数，如587、465等
SMTP_USER=your_email@qq.com
SMTP_PASSWORD=your_email_password  # 邮箱授权码（非登录密码）
FROM_EMAIL=your_email@qq.com
